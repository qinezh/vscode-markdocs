{
	"Insert file inclusion": {
        "prefix": "inclusion",
        "body": "[!include [${1:title}](${2:path})]\n",
		"description": "Insert file inclusion"
    },
    "Insert code snippet": {
        "prefix": "code",
        "body": "[!code-${1:language}[${2:name}](${3:path})]\n",
		"description": "Insert code snippet"        
    },
    "Insert note block": {
        "prefix": "note",
        "body": [
            "> [!NOTE]",
            "> ${1:content}",
            "> ",
            "\n"
        ],
		"description": "Insert note block"        
    },
    "Insert warning block": {
        "prefix": "warning",
