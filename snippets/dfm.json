    "Insert tabbed content": {
        "prefix": "tabbed",
        "body": [
            "# [${1:tab title}](#tab/${2:tab id})",
            "${3:tab content}",
            "# [${4:tab title}](#tab/${5:tab id})",
            "${6:tab content}",
            "***"
        ],
		"description": "Insert tabbed content"        
    }
}